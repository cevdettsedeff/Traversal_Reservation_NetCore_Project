@model GetGuideByIdQueryResult
@{
    ViewData["Title"] = "GetGuide";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Rehber Bilgi Düzenleme Sayfası</h2>
<br />
<form method="post">

    <input type="hidden" asp-for="GuideID" />

    <label>Rehber Adı</label>
    <input type="text" class="form-control" asp-for="Name" />
    <br />

    <label>Rehber Açıklaması</label>
    <input type="text" class="form-control" asp-for="Description" />
    <br />

    <button class="btn btn-outline-success">Bilgileri Güncelle</button>
</form>